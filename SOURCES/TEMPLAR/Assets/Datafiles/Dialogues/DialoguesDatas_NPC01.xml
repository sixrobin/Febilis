<?xml version="1.0" encoding="utf-8" ?>
<DialoguesDatas xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DialoguesDatas.xsd">

   <DialogueDatas Id="NPC01_Greeting">
      <InvertPortraitsAnchors/>
      <Sequence>
         <Sentence Id="NPC01_Greeting_00"/>
         <Pause Dur="0.5"/>
         <Sentence Id="NPC01_Greeting_01"/>
         <Sentence Id="NPC01_Greeting_02"/>
         <Sentence Id="NPC01_Greeting_03"/>
         <Sentence Id="NPC01_Greeting_04"/>
      </Sequence>
   </DialogueDatas>

   <DialogueDatas Id="NPC01_PostGreeting">
      <InvertPortraitsAnchors/>
      <Sequence>
         <Sentence Id="NPC01_PostGreeting_00"/>
      </Sequence>
   </DialogueDatas>

   
   <SentenceDatas Id="NPC01_Greeting_00">
      <Speaker Id="NPC01">
         <OverrideDisplayName>???</OverrideDisplayName>
<!--         <OverridePortraitId>Unknown</OverridePortraitId>-->
      </Speaker>
      <Value>Hello, stranger...</Value>
   </SentenceDatas>

   <SentenceDatas Id="NPC01_Greeting_01">
      <Speaker Id="NPC01">
         <OverrideDisplayName>???</OverrideDisplayName>
<!--         <OverridePortraitId>Unknown</OverridePortraitId>-->
      </Speaker>
      <Value>Interesting.[pause=0.5].[pause=0.5].[pause=0.5] you're from the pale militia. Long time I didn't see this armor.</Value>
   </SentenceDatas>

   <SentenceDatas Id="NPC01_Greeting_02">
      <Speaker Id="NPC01">
         <OverrideDisplayName>???</OverrideDisplayName>
<!--         <OverridePortraitId>Unknown</OverridePortraitId>-->
      </Speaker>
      <Value>I suppose you're heading to the village to inspect all this crazy mess.</Value>
   </SentenceDatas>

   <SentenceDatas Id="NPC01_Greeting_03">
      <Speaker Id="NPC01">
         <OverrideDisplayName>???</OverrideDisplayName>
<!--         <OverridePortraitId>Unknown</OverridePortraitId>-->
      </Speaker>
      <Value>Quick advice, even with your grade, stay low and mind your own business.</Value>
   </SentenceDatas>

   <SentenceDatas Id="NPC01_Greeting_04">
      <Speaker Id="NPC01">
         <OverrideDisplayName>???</OverrideDisplayName>
<!--         <OverridePortraitId>Unknown</OverridePortraitId>-->
      </Speaker>
      <Value>Those are some strange times we're in...</Value>
   </SentenceDatas>

   
   <SentenceDatas Id="NPC01_PostGreeting_00">
      <Speaker Id="NPC01">
         <OverrideDisplayName>???</OverrideDisplayName>
<!--         <OverridePortraitId>Unknown</OverridePortraitId>-->
      </Speaker>
      <Value>Do not forget, stay low and mind your own business.</Value>
   </SentenceDatas>

</DialoguesDatas>