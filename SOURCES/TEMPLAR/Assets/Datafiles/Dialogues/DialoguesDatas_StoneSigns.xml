   <DialogueDatas Id="StoneSign_CatacombsEntry">
      <HidePortraitBox/>
      <Sequence>
         <Sentence Id="StoneSign_CatacombsEntry_Sentence_00"/>
      </Sequence>
   </DialogueDatas>


   <SentenceDatas Id="StoneSign_00_Sentence_00">
      <Speaker Id="Templar">
         <HideSpeakerName/>
      </Speaker>
      <Value>"This is the place of the heretics, who may be forgotten, and so should their names."</Value>
   </SentenceDatas>

   <SentenceDatas Id="StoneSign_01_Sentence_00">
      <Speaker Id="Templar">
         <HideSpeakerName/>
      </Speaker>
      <Value>"This is the place of the ones who built the catacombs."</Value>
   </SentenceDatas>

   <SentenceDatas Id="StoneSign_01_Sentence_01">
      <Speaker Id="Templar">
         <HideSpeakerName/>
      </Speaker>
      <Value>"Once fated to death sentence, they redeemed themselves by putting up this place."</Value>
   </SentenceDatas>

   <SentenceDatas Id="StoneSign_02_Sentence_00">
      <Speaker Id="Templar">
         <HideSpeakerName/>
      </Speaker>
      <Value>"Place of the commoners."</Value>
   </SentenceDatas>

   <SentenceDatas Id="StoneSign_CatacombsEntry_Sentence_00">
      <Speaker Id="Templar">
         <HideSpeakerName/>
      </Speaker>
      <Value>"Here lie the dead, and those who got cursed by Febilis."</Value>
   </SentenceDatas>
   
</DialoguesDatas>